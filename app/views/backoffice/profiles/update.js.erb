<% unless @profile.errors.any? %>
  $('div.errors').html('');
  $('tr#profile-row-<%= @profile.id %>').html("<%= j render partial: 'backoffice/profiles/shared/profile_row', locals: {profile: @profile} %>");
  $('#modal').modal('hide');
<% else %>
  $('div.errors').html('').append("<%= j render :partial => 'backoffice/profiles/shared/profile_errors', locals: {profile: @profile} %>");
<% end %>

<% if notice.present? %>
  toastr["success"]("<%= notice %>");
<% else %>
  toastr["error"]("<%= alert %>");
<% end %>

