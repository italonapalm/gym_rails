
<div class="row">
  <div class="col-md-12">
    <div class="panel panel-white">
      <div class="panel-heading clearfix">
          <h4 class="panel-title">Cadastro de Academias</h4>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-md-12">
            <%= link_to '#', { class: 'btn btn-success', 'data-toggle': "modal", 'data-target': ".bs-example-modal-lg" } do %>
              New Gym Using Ajax
            <% end %>

            <%= render partial: 'backoffice/gyms/shared/form_modal' %>

          </div>
          <div class="col-md-12">
            <div class="table-responsive">
              <table class="table table-striped table-hover">
                <thead>
                  <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Actions</th>
                  </tr>
                </thead>
                <tbody id="gyms-table">
                  <%= render partial: 'backoffice/gyms/shared/gym_table' %>

                </tbody>
              </table>
            </div>
          </div>


        </div>

      </div>
    </div>
  </div>


</div>

